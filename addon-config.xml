<!-- 
  use this addon-config.xml to create/set/publish your property tree
  or override default settings from defaults.xml

  Node: this file MUST be named addon-config.xml and it MUST be in the
  root path of your addon/plugin.
  The topmost element MUST be <PropertyList>
-->               
        
<PropertyList>
    <addons>
        <by-id>
            <org.flightgear.addons.landing-challenge>
                <addon-devel>
                    <airport type="string">EDHE</airport>
                    <runway type="string">09</runway>
                    <target-dist-m type="int">90</target-dist-m>
                    <markings-offset-m type="int">0.5</markings-offset-m>
                    <landed>0</landed>
                    <altTrig>0</altTrig>
                    <landing-data>
                        <g-force type ="string"></g-force>
                        <fpm type = "string"></fpm>
                        <overshoot type="string"></overshoot>
                        <offcenter type="string"></offcenter>
                        <bank-angle type="string"></bank-angle>
                        <slip-angle type="string"></slip-angle>
                    </landing-data>                
                </addon-devel>
            </org.flightgear.addons.landing-challenge>
        </by-id>
    </addons>
</PropertyList>
